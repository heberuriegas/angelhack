%div.navbar.navbar-inverse
  %div.navbar-inner
    %ul.nav
      -if user_signed_in?
        %li
          %a{:href =>"#"}
            ="Welcome #{current_user.email}"
        %li=link_to 'Sign out', destroy_user_session_path, method: :delete, confirm: 'Are you sure you want to log out?'
